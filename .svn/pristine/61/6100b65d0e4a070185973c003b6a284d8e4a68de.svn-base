<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>

<table class="col-10 table-bordered">
	<thead>
		<tr>
			<th>회의실 번호</th>
			<th>회의실 이름</th>
			<th>회의실 위치</th>
			<th>수용 인원</th>
			<th>회의실 상태</th>
			<th>회의실 이용안내</th>
		</tr>
	</thead>
	<tbody>
		<c:choose>
			<c:when test="${not empty meetingroomList }">
				<c:forEach items="${meetingroomList }" var="meetingroom">
					<tr>
						<td></td>
					</tr>
				</c:forEach>
			</c:when>
		</c:choose>
	</tbody>
</table>