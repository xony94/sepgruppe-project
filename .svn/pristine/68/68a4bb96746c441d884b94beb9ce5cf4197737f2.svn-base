<!-- 
 * == 개정이력(Modification Information) ==
 *   
 *   수정일      			수정자           수정내용
 *  ============   	============== =======================
 *  2025. 3. 21.     	KMJ            최초 생성
 *
-->
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<table class="table">
    <thead>
        <tr>
            <th scope="col"></th>
            <th>아이디</th>
            <th>일정명</th> 
            <th>일정설명</th>
            <th>일정시작일</th>
            <th>일정종료일</th>
            <th>일정장소</th>
            <th>등록일시</th>
            <th>일정상태</th>
            <th>수정일시</th>
        </tr>
    </thead>
    <tbody>
        <c:forEach var="schedule" items="${schedule}">
            <tr>
                <td>
                    <input type="radio" name="schdlEdit" value="${schedule.empId}" class="schedule-radio"
                        data-id="${schedule.empId}"
                        data-name="${schedule.schdlNm}"
                        data-description="${schedule.schdlCn}"
                        data-start="${schedule.schdlBgngYmd}"
                        data-end="${schedule.schdlEndYmd}"
                        data-location="${schedule.schdlLocation}"
                        data-createdate="${schedule.schdlCreateDate}"
                        data-status="${schedule.schdlStatus}"
                        data-updatedate="${schedule.schdlUpdateDate}">
                </td>
                <td>${schedule.empId}</td>
                <td>${schedule.schdlNm}</td>
                <td>${schedule.schdlCn}</td>
                <td>${schedule.schdlBgngYmd}</td>
                <td>${schedule.schdlEndYmd}</td>
                <td>${schedule.schdlLocation}</td>
                <td>${schedule.schdlCreateDate}</td>
                <td>${schedule.schdlStatus}</td>
                <td>${schedule.schdlUpdateDate}</td>
            </tr>
        </c:forEach>
    </tbody>
</table>


